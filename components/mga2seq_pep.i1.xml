<?xml version="1.0" encoding="UTF-8"?>

<commandSetRoot xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation='commandSet.xsd'>
    <commandSet type="serial">
        <name>mga2seq_pep compute</name>
        <command>
            <type>RunUnixCommand</type>
            <name>mga2seq_pep</name>
            <state>incomplete</state>
            <executable>$;MGA2SEQ_PEP_EXEC$;</executable>
            <arg>-i $;I_FILE_PATH$; -m $;MGA_RAW_LIST$;</arg>
            <param>
                <key>--outdir</key>
                <value>$;OUTPUT_DIRECTORY$;/$;ITERATOR_NAME$;/g$;GROUP_NUMBER$;</value>
            </param>
            <param>
                <key>--prefix</key>
                <value>$;I_FILE_BASE$;</value>
            </param>
            <param>
                <key>stderr</key>
                <value>$;OUTPUT_DIRECTORY$;/$;ITERATOR_NAME$;/g$;GROUP_NUMBER$;/$;I_FILE_BASE$;.$;COMPONENT_NAME$;.stderr</value>
            </param>
            <param>
                <key>stdout</key>
                <value>$;OUTPUT_DIRECTORY$;/$;ITERATOR_NAME$;/g$;GROUP_NUMBER$;/$;I_FILE_BASE$;.$;COMPONENT_NAME$;.stdout</value>
            </param>
        </command>
    </commandSet>
</commandSetRoot>
