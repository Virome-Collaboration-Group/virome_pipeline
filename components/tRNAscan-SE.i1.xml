<?xml version="1.0" encoding="UTF-8"?>

<commandSetRoot xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation='commandSet.xsd'>
    <commandSet type="serial">
        <name>tRNAscan-SE compute</name>
        <command>
            <type>RunUnixCommand</type>
            <name>tRNAscan-SE</name>
            <state>incomplete</state>
            <executable>export TMPDIR=$;TMP_DIR$;; perl -I $;TRNASCANSE_INSTALL$; $;TRNASCANSE_EXEC$;</executable>
            <arg>-q $;OTHER_OPTS$; -b $;SEARCH_TYPE$; -o $;OUTPUT_DIRECTORY$;/$;ITERATOR_NAME$;/g$;GROUP_NUMBER$;/$;I_FILE_BASE$;.$;COMPONENT_NAME$;.txt $;I_FILE_PATH$;</arg>            
            <param>
                <key>stdout</key>
                <value>$;TMP_DIR$;/$;I_FILE_BASE$;.$;COMPONENT_NAME$;.stdout</value>
            </param>
            <param>
                <key>stderr</key>
                <value>$;TMP_DIR$;/$;I_FILE_BASE$;.$;COMPONENT_NAME$;.stderr</value>
            </param>
        </command>
    </commandSet>
</commandSetRoot>
