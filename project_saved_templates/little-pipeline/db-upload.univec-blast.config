[interface]
classification = virome

[parameter]
;; location of sqlite3 database
$;DATABASE_FILE$;= $;REPOSITORY_ROOT$;/output_repository/init-db/$;PIPELINEID$;_default/processing.sqlite3
;; valid values are sequence, blastp, blastn, blastx or tRNA
$;TABLE_NAME$; = blastn

[input]
;; prepared output file for import
$;INPUT_FILE_LIST$; = $;REPOSITORY_ROOT$;/output_repository/blast-result-prep/$;PIPELINEID$;_univec/blast-result-prep.txt.list
$;INPUT_FILE$; =
$;INPUT_DIRECTORY$; =
;; the following is only used when iterating over an INPUT_DIRECTORY
$;INPUT_EXTENSION$; = raw

[output]
$;OUTPUT_TOKEN$; = univec-blast
$;OUTPUT_DIRECTORY$; = $;REPOSITORY_ROOT$;/output_repository/$;COMPONENT_NAME$;/$;PIPELINEID$;_$;OUTPUT_TOKEN$;

[component]
$;COMPONENT_NAME$; = db-upload
$;DESCRIPTION$; = none
$;WORKFLOW_REPOSITORY$; = $;REPOSITORY_ROOT$;/workflow/runtime/$;COMPONENT_NAME$;/$;PIPELINEID$;_$;OUTPUT_TOKEN$;
$;PIPELINE_TOKEN$; = unnamed

;The version,revision,tag here is set by an interpolated CVS tag
$;VERSION$; = 1.0
$;RELEASE_TAG$; = $Name$
$;REVISION$;    = $Revision: 1 $

$;TEMPLATE_XML$; = $;DOCS_DIR$;/$;COMPONENT_NAME$;.xml
$;ITERATOR1$; = i1
$;ITERATOR1_XML$; = $;DOCS_DIR$;/$;COMPONENT_NAME$;.$;ITERATOR1$;.xml

;Distributed options
$;GROUP_COUNT$; = 150
$;NODISTRIB$; = 1

;the following keys are replaced at runtime by the invocation script
$;COMPONENT_CONFIG$; =
$;COMPONENT_XML$; =
$;PIPELINE_XML$; =
$;PIPELINEID$; =

[include]
$;PROJECT_CONFIG$;=

[dce]
;; These configuration options only apply to any distributed components
$;GROUP$;=
$;OS$;=
$;MEMORY$;=1
$;HOSTS$;=
$;QUEUE$;=workflow.q
$;DURATION$;=
$;PRIORITY$;=
$;PASSTHROUGH$;=
